#JSGF V1.0;

/**
 * JSGF Grammar to move the robot
 */

grammar voice_cmd;

public <simple_command> = (ROBOT TAKE THE <object> TO THE <location>);
<object> = PRINGLES | DRINK;
<location> = TABLE | KITCHEN;

