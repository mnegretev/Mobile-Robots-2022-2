#JSGF V1.0;

/**
 * JSGF Grammar similar to kws example
 */

grammar voice_cmd;

public <move> = MOVE FORWARD;

public <move2> = ROBOT | OKEY <verb> [TO THE] [<object>] <location>;

<verb> = TAKE | MOVE;

<speed> = FULL SPEED | HALF SPEED;

<object> = PRINGLES | DRINK;

<location> = KITCHEN | TABLE;
